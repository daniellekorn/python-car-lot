import datetime


def add_to_log(msg):
    try:
        f = open("log.txt", "a")
    except Exception as e:
        print("Something went wrong: " + str(e))
    else:
        x = datetime.datetime.now()
        f.write(x.strftime("%d/%m/%y %H:%M:%S ") + msg + "\n")


add_to_log("test")
add_to_log("test two")
add_to_log("test three")